                <h6 class="mb-0 mt-3">{{testimonial3.name}}</h6>
                <span class="small">{{testimonial3.position}}</span>
              </div>

            </div>
          </div>
        </div>
      </div><!-- row END -->
    </div>
  </section>
  <!-- =======================
  Testimonials END -->

  <!-- =======================
  Portfolio START -->
  <section id="portfolio" class="pt-4">
    <div class="container">
      <!-- Title -->
      <div class="row d-flex justify-content-between align-items-center">
        <div class="col-sm-8 mb-3">
          <h2 class="display-5 mb-0 mb-sm-4 text-dark-stroke">{{sectionTitle}}</h2>
        </div>
        <div class="col-sm-4 mb-4 text-start text-sm-end">
          <a href="{{buttonLink}}" class="btn btn-line text-dark mt-3">{{buttonText}}</a>
        </div>
      </div>
      <!-- Portfolio items -->
      <div class="row">
        <div class="col-lg-12">
          <div class="tiny-slider dots-bordered">
            <div class="tiny-slider-inner" data-arrow="false" data-dots="true" data-items-xl="3" data-items-lg="3" data-items-md="3" data-items-sm="2" data-items-xs="1">

              {{#each portfolioItems}}
                <div class="item">
                  <a href="{{link}}" class="card card-metro">
                    <!-- Card Image -->
                    <div class="card-image">
                      <img src="{{imageSrc}}" alt="card image">
                    </div>
                    <!-- Card Overlay -->
                    <div class="card-img-overlay d-flex flex-column">
                      <img class="w-40" src="{{clientLogo}}" alt="">
                      <div class="mt-auto card-text">
                        <h5 class="text-white">{{title}}</h5>
                        <p class="small text-white mb-0 text-truncate">{{description}}</p>
                      </div>
                    </div>
                  </a>
                </div>
              {{/each}}

            </div>
          </div>
        </div>
      </div><!-- row END -->
    </div>
  </section>
  <!-- =======================
  Portfolio END -->

  <!-- =======================
  Steps START -->
  <section id="about">
    <div class="container">
      <div class="row">
        <!-- Step 1 -->
        <div class="col-sm-12 col-md-4 mb-5">
          <div class="text-start position-relative">
            <h2 class="display-4 mb-0 text-light-stroke text-light-shadow">01</h2>
            <h4 class="position-absolute start-0 top-0 mt-0 mt-sm-3 ps-6 d-block">
              {{{step1_title}}}
            </h4>
          </div>
          <div class="ps-6 pt-4 pt-sm-0">
            <p>{{step1_desc}}</p>
            <img class="rounded" src="assets/images/about/01.jpg" alt="">
          </div>
        </div>

        <!-- Step 2 -->
        <div class="col-sm-6 col-md-4 mb-5">
          <div class="text-start position-relative">
            <h2 class="display-4 mb-0 text-light-stroke text-light-shadow">02</h2>
            <h4 class="position-absolute start-0 top-0 mt-0 mt-sm-3 ps-6 d-block">
              {{{step2_title}}}
            </h4>
          </div>
          <div class="ps-6 pt-4 pt-sm-0">
            <p>{{step2_desc}}</p>
            <img class="rounded" src="assets/images/about/02.jpg" alt="">
          </div>
        </div>

        <!-- Step 3 -->
        <div class="col-sm-6 col-md-4">
          <div class="text-start position-relative">
            <h2 class="display-4 mb-0 text-light-stroke text-light-shadow">03</h2>
            <h4 class="position-absolute start-0 top-0 mt-0 mt-sm-3 ps-6 d-block">
              {{{step3_title}}}
            </h4>
          </div>
          <div class="ps-6 pt-4 pt-sm-0">
            <p>{{step3_desc}}</p>
            <img class="rounded" src="assets/images/about/03.jpg" alt="">
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- =======================
  Steps END -->

  <!-- =======================
  Clients START -->
  <section class="pt-0">
    <div class="container">
      <div class="row">
        <!-- Title -->
        <div class="col-lg-4">
          <h2 class="display-5 mb-3">{{clients_title}}</h2>
          <p>{{clients_desc}}</p>
        </div>

        <!-- Clients logos -->
        <div class="col-lg-8">
          <div class="row">
            {{#each client_logos}}
              <div class="col-6 col-sm-4 col-lg-3">
                <div class="mb-4 p-4 grayscale bg-light-overlay-dotted text-center">
                  <img src="{{this}}" alt="">
                </div>
              </div>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- =======================
  Clients END -->

