    <div class="container">
      <div class="row g-0 d-flex align-items-center">
        <!-- Our Work Block -->
        <div class="col-sm-6">
          <div class="text-center bg-primary-multiply position-relative overflow-hidden px-3 py-6 h-100">
            <h2 class="mb-3 display-6">
              <a class="text-white-stroke" href="#">{{our_work_title}}</a>
            </h2>
            <a class="btn btn-line text-white mb-0" href="#">{{our_work_button}}</a>
            <img
              class="bg-primary-multiply position-absolute top-50 start-50 translate-middle z-index-n9"
              src="{{our_work_image}}" alt="">
          </div>
        </div>

        <!-- Project Block -->
        <div class="col-sm-6">
          <div class="text-center bg-dark px-3 py-6 h-100">
            <h2 class="mb-3 display-6 text-white">{{project_block_title}}</h2>
            <a class="btn btn-line text-white mb-0" href="#">{{project_block_button}}</a>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- =======================
  Action box END -->

  <!-- =======================
  Newsletter START-->
  <section class="bg-dark position-relative bg-dark-overlay-4">
    <div class="container">
      <div class="row justify-content-between">
        <!-- Title -->
        <div class="col-sm-5">
          <h2 class="display-6 text-white-stroke">{{newsletter_title}}</h2>
        </div>

        <!-- Content and input -->
        <div class="col-sm-6">
          <p class="mt-3">{{newsletter_description}}</p>
          <form>
            <div class="input-group">
              <input
                class="form-control bg-white border-0"
                type="text"
                name="Newsletter"
                placeholder="{{newsletter_input_placeholder}}"
              />
              <button type="button" class="btn btn-line btn-primary mb-0">
                {{newsletter_button}}
              </button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>
  <!-- =======================
  Newsletter END-->

</main>
<!-- **************** MAIN CONTENT END **************** -->

